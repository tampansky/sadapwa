sleep 0.25
    echo -e $green"_\ \ (_| | (_| | (_| | |_) |  \  /\  / (_| |"
sleep 0.25
    echo -e $green"\__/\__,_|\__,_|\__,_| .__/    \/  \/ \__,_|"
sleep 0.25
