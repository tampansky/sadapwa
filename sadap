    sleep 0.25
    sleep 3
    echo -e $okegreen"Melanjutkan Untuk sadap wa ? y/n"$okegreen
    read -p ">" Dump
    if [ $Dump = "y"  ]; then 
    sha 
    else
    echo -e "kesalahan"
    sleep 1 
    clear
    fi 
    sha(){
    sleep 1
    echo -e $okegreen" SADAP WA ACTIVED"
    sleep 2
    echo -e $okegreen"1.)Helky"
    sleep 0.25
    echo -e $okegreen"2.)S1XT3N"
    sleep 0.25
    echo -e $okegreen"3.)PSCT officiall"
    sleep 0.25
    echo -e $okegreen"4.)D45H7 officiall"
    sleep 0.25
    echo -e $okegreen"PILIH CHAT!!!"
    echo -n -e ">"
    read -e lol
    case $lol in
    ("1")
    helky
    ;;
    ("2")
     sis
     ;;
     ("3")
     psct
     ;;
     ("4")
     sid
     ;;
    esac
    }
