    read -p ">" u
    echo -e $green"Menganalisa..." 
    sleep 2
    clear
    echo -e $green"NOMOR BIASA : $u"
    echo -e $green"NOMOR WA : +62$u"
    sleep 1
    echo
    echo -e "1.Dump Chat"
    echo -e "2.Keluar"
    read -p ">" p 
    if [ $p = "1" ]; then 
    echo -e $green "SEDANG MASA PERBARUAN TQ."
    else
    echo "SYNTAX EROR 404"
    fi 
    mulai

    if [ $p = "2" ]; then
    echo -e $green"Thanks To Play My Tools"
    else
    echo "SYNTAX EROR 404"
    fi
    mulai
    
}
