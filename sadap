#INTRO
intro(){
    clear
    echo -e $okegreen" __           _               __    __      "
sleep 0.25
    echo -e $okegreen"/ _\ __ _  __| | __ _ _ __   / / /\ \ \__ _ "
sleep 0.25
    echo -e $okegreen"\ \ / _  |/ _  |/ _  |  _ \  \ \/  \/ / _  |"
sleep 0.25
    echo -e $okegreen"_\ \ (_| | (_| | (_| | |_) |  \  /\  / (_| |"
sleep 0.25
    echo -e $okegreen"\__/\__,_|\__,_|\__,_| .__/    \/  \/ \__,_|"
sleep 0.25
    echo -e $okegreen"                     |_|                    "
sleep 1
    echo -e $okegreen"||========================================||"
    sleep 0.25
    echo -e $okegreen"||=WELCOME TO TOOLS SADAP WA BY Tampansky=||"
    sleep 0.25
    echo -e $okegreen"||========================================||"
    sleep 0.25
}

#outro
    intro
    sleep 1
    echo -e $okegreen        "    Author : Tampansky   "
    sleep 0.25
